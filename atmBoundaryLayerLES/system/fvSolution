FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
   location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          PCG;

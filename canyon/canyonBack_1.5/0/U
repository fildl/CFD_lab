FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.05 0 0);

boundaryField
{
    inflow
    {
         type       cyclic;
    }
    outflow
    {
         type       cyclic;
    }
    front
    {
         type       empty;
    }
    back
    {
        type        empty;
    }
    sky
    {
        type        fixedValue;
        value       uniform (20 0 0);
    }
    bldgFront
    {
        type        noSlip;
    }
    bldgBack
    {
        type        noSlip;
    }
    street
    {
        type        noSlip;
    }
}

// ************************************************************************* //

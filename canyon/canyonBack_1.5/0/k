FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.001;

boundaryField
{
    inflow
    {
         type       cyclic;
    }
    outflow
    {
         type       cyclic;
    }
    front
    {
         type       empty;
    }
    back
    {
        type        empty;
    }
    sky
    {
        type            zeroGradient;
    }
    bldgFront
    {
        type            nutkWallFunction;
        value           uniform 0.375;
    }
    bldgBack
    {
        type            nutkWallFunction;
        value           uniform 0.375;
    }
    street
    {
        type            nutkWallFunction;
        value           uniform 0.375;
    } 
}



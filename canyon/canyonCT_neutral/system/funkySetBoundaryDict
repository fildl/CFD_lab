{
    field c;
    expressions
    (
        {
            patchName   street;
            target      gradient;
            //variables   "Z=pos().z;X=pos().x";
            //expression  "vector(1,0,0)*( 0.902439*log( mag(altezza)/z0 ) )";
            expression  "(pos().x>0.65 && pos().x<1.35) ? (  0.7142857e05 ) : ( 0 )";
        }
    );

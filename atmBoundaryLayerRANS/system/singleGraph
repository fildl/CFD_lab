start   (-295 0 0);
end     (-295 0 500);
nPoints 1000;

fields  (U k epsilon);

axis    distance; // The independent variable of the graph. Can be "x",
                 // "y", "z", "xyz" (all coordinates written out), or
                 // "distance" (from the start point).

